          </div>


          <div class="testimonials owl-carousel block">
            <!-- Testimonial -->

             <% profile.testimonials.forEach(function(singleItem){ %>
                <div class="testimonial-item">
                  <!-- Testimonial Author -->
                  <div class="testimonial-credits">
                    <!-- Picture -->
                    <%
                      getTestimonialImageUrl = url => {
                        let urlArray = url.split('upload');
                        let finalUrl = urlArray[0] + 'upload/w_200,h_200' + urlArray[1];
                        return finalUrl;
                      };
                    %>
                    <div class="testimonial-picture">
                      <img src="<%= getTestimonialImageUrl(singleItem.img) %>" alt="">
                    </div>
                    <!-- /Picture -->
                    <!-- Testimonial author information -->
                    <div class="testimonial-author-info">
                      <p class="testimonial-author"><%= singleItem.name %></p>
                      <p class="testimonial-firm"><%= singleItem.job %></p>
                    </div>
                  </div>
                  <!-- /Testimonial author information -->
                  <!-- Testimonial Content -->
                  <div class="testimonial-content">
                    <div class="testimonial-text">
                      <p>"<%= singleItem.testimonial %>"</p>
                    </div>
                  </div>
                  <!-- /Testimonial Content -->
